{
  "appenders": {
    "out": {
      "type": "stdout"
    },
    "logfile": {
      "type": "dateFile",
      "filename": "log/logfile.log",
      "pattern": "-yyyy-MM-dd",
      "maxLogSize": 10485760,
      "backups": 5,
      "compress": true
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "out",
        "logfile"
      ],
      "level": "info"
    }
  }
}